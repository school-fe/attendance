{
  "presets": ["es2015", "stage-0", "react"],
  "plugins": [
    "transform-runtime",
    "syntax-dynamic-import",
    "transform-object-assign",
    "transform-async-to-generator",
    "transform-decorators-legacy",
    "syntax-trailing-function-commas",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true
      }
    ]
  ]
}
